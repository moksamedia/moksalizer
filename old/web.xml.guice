<web-app>
     <listener>
       <listener-class>com.moksamedia.moksalizer.servlet.MoksalizerGuiceContextListener</listener-class>
     </listener>
	<filter>
       <filter-name>Guice Filter</filter-name>
       <filter-class>com.google.inject.servlet.GuiceFilter</filter-class>
	</filter>
     <filter-mapping>
       <filter-name>Guice Filter</filter-name>
       <url-pattern>/*</url-pattern>
     </filter-mapping>
</web-app>